    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "playroom": "playroom start",
    "playroom:build": "playroom build"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@types/styled-components": "^5.1.0",
    "mui-primitives": "0.0.10-0",
